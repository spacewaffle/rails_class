
<h1>
  <span id="drawing"></span>
  <script>
    var draw = SVG('drawing').fixSubPixelOffset().size(55, 80)

    var hex = draw.polygon().ngon({
               radius: 23 
             , edges:    6
             }).move(6, 35).fill({ opacity: 0 }).stroke({ color: 'rgb(99, 236, 167)', opacity: 0, width: 8 })
    var rail = draw.rect(10, 40).move(27, 0).rotate(-50).skew(-50, 0).fill({ color: '#ccc' })

    var cover = draw.polyline('7,26 26,14 45,26').fill('none').stroke({ color: 'rgb(99, 236, 167)', width: 8, opacity: 0 }).move(6, 35)

    var rail2 = draw.rect(10, 40).move(15,0) .rotate(50).skew(50, 0).fill({ color: '#ccc' })

    var cover2 = draw.polyline('7,25 26,14').fill('none').move(26, 70).rotate(180).stroke({ color: 'rgb(99, 236, 167)', width: 8, opacity: 0 })

    var drop, raise;
    drop = function() {
        setTimeout(function() {
          hex.animate(500, SVG.easing.backout).move(6, 35).stroke({ opacity: 0 }).after(raise)
          cover.animate(500, SVG.easing.backout).move(6, 35).stroke({ opacity: 0 }).after(raise)
          cover2.animate(500, SVG.easing.backout).move(26, 70).stroke({ opacity: 0 }).after(raise)
        }, 5000)
      }
    raise = function() {
      setTimeout(function() {
        hex.animate(1000, SVG.easing.bounce).move(6, 15).stroke({ opacity: 1 }).after(drop)
        cover.animate(1000, SVG.easing.bounce).move(6, 15).stroke({ opacity: 1 }).after(drop)
        cover2.animate(1000, SVG.easing.bounce).move(26, 90).stroke({ opacity: 1 }).after(drop)

      }, 500);
    }
    raise();

  </script>
